escape() {
    yes(/^\|\(\)\^\$\.\*\+\?$/, "|()^$.*+?")
    no(/^\|\(\)\^\$\.\*\+\?$/, "aa")
    no(/^\\$/, "a\\b")
    yes(/^\\$/, "\\")
    yes(/\\/, "a\\b")
    no(/^\\$/, "a\\b")
    yes(/\a/, "a")
    no(/^\a$/, "ba")
}

