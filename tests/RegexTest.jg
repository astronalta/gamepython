# jgtest exec on


yes(re Regex::Regex, str String) {
    if not (re =~ str) {
        re.dump()           
    } else {
    }
}

no(re Regex::Regex, str String) {
    if re =~ str {
        Io::println("fail #{str}")
        re.dump()           
    } else {
    }
}

