    no(/ba*/, 'x')
    no(/ba*/, '')

    i var = 0
    str var = 'b'
    while i < 1000 {
        str = str + 'a*'
        ++i
    }

    re = Regex::Regex(str)
    no(re, '')
    yes(re, 'b')
