# aptest exec on

TestComp1 < Object {
    var1 Int = 42
    share Int 

    test1() String {
        ret "hello\n"
    }
}

TestComp2 < Object {
    var2 Int = 99
    share Int

    test2() String {
        ret "goodbye\n"
    }
}

TestClass < Object {
    TestComp1
    TestComp2
}

TestInterface < Interface {
    test1() String
    test2() String
}

main() Int {
    Io::print(TestClass().test1())
    Io::print(TestClass().test2())
    Io::println(TestClass().var1.str)
    Io::println(TestClass().var2.str)

    obj TestInterface = TestClass()
    Io::print(obj.test1())
    Io::print(obj.test2())
    Io::stdout().flush()
    TestClass().hash.str
    ret 0 
}
