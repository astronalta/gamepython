# aptest exec on

main() Int {
    a = true
    b = false
    
    # Test true/false branching with no nesting
    if a {
        Boot::print_str("true\n")
    } else {
        Boot::print_str("false\n")
    }
    if b {
        Boot::print_str("true\n")
    } else {
        Boot::print_str("false\n")
    }

    # Nested branching test #1
    Boot::print_str("1\n")
    if a {
        Boot::print_str("2\n")
        if a {
            Boot::print_str("3\n")
        } else {
            Boot::print_str("fail\n")
        }
        Boot::print_str("4\n")
    } else {
        Boot::print_str("fail\n")
    }
    Boot::print_str("5\n")

    
    # Nested branching test #2
    Boot::print_str("6\n")
    if b {
        if a {
            Boot::print_str("fail\n")
        } else {
            Boot::print_str("fail\n")
        }
        Boot::print_str("fail\n")
    } else {
        Boot::print_str("7\n")
        if a {
            Boot::print_str("8\n")
        } else {
            Boot::print_str("fail\n")
        }
        Boot::print_str("9\n")
    }
    Boot::print_str("10\n")
    

    ret 0
}
