
import Boot, Coroutine

countdown(num Int) {
    if num != 0 {
        countdown(num-1)
    }
}

main() Int {

    coroutine = Coroutine() func() {
        i = 0
    
        nested = Coroutine() func() {
            countdown(40960) # Check to make sure the stack grows
            boot_print_str('hello ')
            yield
            boot_print_str('world\n')
        }
        nested()
        nested()
        nested()
        nested()

        while true {
            str = String()
            i = i + 1
            boot_print_int(i)
            boot_print_str('\n')
            yield
        }
       
    }
    coroutine()
    coroutine()
    coroutine()
    coroutine()
    coroutine()
    coroutine()
    coroutine()
    coroutine()
    
    ret 0
}
