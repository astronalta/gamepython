import Boot

TestObject < Object {

    initialized = 137
    bar Int
    baz Bool

    @init() {
        bar = 99
        baz = true
    }

    bar?() Int { 
        ret bar
    }

    baz?() Bool {
        ret baz
    }

    initialized?() Int {
        ret initialized
    }
}

main() Int {
    test = TestObject()
    
    boot_print_int(test.bar?())
    boot_print_str("\n")
    if test.baz?() {
        boot_print_str("true\n")
    } else {
        boot_print_str("false\n")
    }
    boot_print_int(test.initialized) 
    boot_print_str("\n")
    ret 0
}
