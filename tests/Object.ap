import Boot

TestObject < Object {

    bar Int
    baz Bool

    @init() {
        bar = 99
        baz = true
    }

    bar?() Int { 
        ret bar
    }

    baz?() Bool {
        ret baz
    }
}

main() Int {
    test = TestObject()
    
    boot_print_int(test.bar?())
    boot_print_str("\n")
    if test.baz?() {
        boot_print_str("true\n")
    } else {
        boot_print_str("false\n")
    }
    ret 0
}
