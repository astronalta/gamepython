        Io::println("Fail: Non-unique auto-assigned constants")
    }
    if (Manual::MANGO == Manual::PEACH) {
        Io::println("Fail: Non-unique auto-assigned constants")
    }
    if (Manual::MANGO == Manual::PEAR) {
        Io::println("Fail: Non-unique auto-assigned constants")
    }
