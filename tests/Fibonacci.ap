# aptest exec on

fib(n Int) Int {
    if n == 0 { 
        ret 0
    }
    if n == 1 {
        ret 1
    }
    ret fib(n-1) + fib(n-2)
}


main() Int {
    x = fib(0)
    Boot::print_int(x)   
    Boot::print_str("\n")
    x = fib(1)
    Boot::print_int(x)   
    Boot::print_str("\n")
