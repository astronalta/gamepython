# aptest exec on

conditional1() Int {
    a = true
    if a {
        Boot::print_str("one\n")
    } else {
        Boot::print_str("X\n")
    }
    Boot::print_str("two\n")
    ret 0
}

conditional2() Int {
    a = true
    b = false
    if a {
        Boot::print_str("three\n")
    } else if b {
        Boot::print_str("X\n")
    } else {
        Boot::print_str("X\n")
    }
    Boot::print_str("four\n")
    ret 0
}

conditional3() Int {
    a = true
    b = false
    if b {
        Boot::print_str("X\n")
    } else {
        if b {
            Boot::print_str("X\n")
        } else if a {
            Boot::print_str("five\n")
        } else {
            Boot::print_str("X\n")
        }
        Boot::print_str("six\n")
    }
    Boot::print_str("seven\n")
    ret 0
}

main() Int {
    conditional1()
    conditional2()
    conditional3()
    ret 0
}
