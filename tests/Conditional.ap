class Conditional {


    def conditional1():Integer {
        if (true) {
            return 1
        } else {
            return 0
        }
    }

    def conditional2():Integer {
        if (true) {
            return 1
        } else if (false) {
            return 2
        } else {
            return 3
        }
    }

    def conditional3():Integer {
        if (true) {
            return 1
        } else {
            if (true) {
                return 2
            } else if (false) {
                return 4
            } else {
                return 6
            }
        }
    }
}
