class Conditional {

    conditional1():Integer {
        if (true) {
            return 1
        } else {
            return 0
        }
    }

    conditional2():Integer {
        if (true) {
            return 1
        } else if (false) {
            return 2
        } else {
            return 3
        }
    }

    conditional3():Integer {
        if (true) {
            return 1
        } else {
            if (true) {
                return 2
            } else if (false) {
                return 4
            } else {
                return 6
            }
        }
    }
}
