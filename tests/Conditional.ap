Conditional < Object {

    conditional1() Int {
        if true {
            return 1
        } else {
            return 0
        }
    }

    conditional2() Int {
        if true {
            return 1
        } else if false {
            return 2
        } else {
            return 3
        }
    }

    conditional3() Int {
        if true {
            return 1
        } else {
            if true {
                return 2
            } else if false {
                return 4
            } else {
                return 6
            }
        }
    }
}
