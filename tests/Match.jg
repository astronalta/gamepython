    #}
    match any {
    with x String:
    with y String:
    with *:
    }
}

main() Int {

    switch(Fruit::APPLE)
    switch(Fruit::ORANGE)
    switch(Fruit::PEAR)
    switch(Fruit::BANANA)
    switch(Fruit::STRAWBERRY)
    switch(Fruit::KIWI)
    
    switch2(Fruit::APPLE)
    switch2(Fruit::ORANGE)
    switch2(Fruit::PEAR)
    switch2(Fruit::BANANA)
    switch2(Fruit::STRAWBERRY)

    switch_str('apple')
    switch_str('orange')
    switch_str('pear')
    switch_str('banana')
    switch_str('strawberry')

    ret 0
}
