# jgtest exec on

import Io

Fruit = APPLE | ORANGE | PEAR | BANANA | STRAWBERRY | KIWI

switch(fruit Fruit) {
    match fruit {
    with Fruit::APPLE: print('apple') 
    with Fruit::ORANGE: print('orange')
    with Fruit::PEAR: print('pear')
    with Fruit::BANANA: print('banana') 
    with Fruit::STRAWBERRY: print('strawberry')
    with *: print('unknown')
    }
    println('.')
}

switch2(fruit Fruit) {
    match fruit {
    with Fruit::APPLE: 
        print('apple') 
        print('1')
    with Fruit::ORANGE: 
        print('orange')
        print('2')
    with Fruit::PEAR: 
        print('pear')
        print('3')
    with Fruit::BANANA: 
        print('banana') 
        print('4')
    with *: 
        print('unknown')
        print('5')
    }
    println('.')
}

switch_str(str String) {
    match str {
    with 'apple':
        print('apple')
        print('1')
    with 'orange':
        print('orange')
        print('2')
    with 'pear':
        print('pear')
        print('3')
    with 'banana':
        print('banana')
        print('4')
    with *: 
        print('unknown')
        print('5')
    }
    println('.')
}

switch_any(any Any) {
    #match any {
    #with 'foo':
