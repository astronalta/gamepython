# jgtest exec on

main() Int {
    count = 0
    fd = File::open('UnicodeFile.txt', 'r')
    fd.peek()
    while fd.status == Io::StreamStatus::OK {
        Utf8::put(Io::stdout(), Utf8::get(fd))
        fd.peek()
        ++count
    }
    Io::stdout().println(count.str)
    Io::stdout().flush()

    fd = File::open('UnicodeFile.txt', 'r')
    str = fd.readall()
    Io::print(str)
    Io::stdout().flush()
    ret 0
}
