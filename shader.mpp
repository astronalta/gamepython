class Jet::OpenGLShader;

CoreEngine engine;
ResourceState state = RenderState::UNLOADED;
String name;
bool enabled;

@init(CoreEngine engine, String name) {
    self.engine = engine;
    self.name = name;
}

@destroy() {
    state(RenderState::UNLOADED);
}

name() String {
    return name;
}

state() ResourceState {
    return state;
}

enabled?() bool{
    return enabled;
}
